	<queryString language="SQL">
		<![CDATA[SELECT t.id_transaksi, t.tanggal, t.berat, t.total, l.jenis_layanan, m.nama_member, m.alamat, m.no_telepon
FROM transaksi t
JOIN member m ON t.member_id_member = m.id_member
JOIN layanan l ON t.layanan_id_layanan = l.id_layanan

]]>
